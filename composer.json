{
    "name": "tomsgu/xutim",
    "description": "Modular CMS system with Symfony, Doctrine ORM, Turbo/Stimulus, and Editor.js. Includes main application and internal bundles/libraries in a monorepo structure.",
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Tomas Jakl",
            "email": "tomasjakll@gmail.com"
        }
    ],
    "require": {
        "babdev/pagerfanta-bundle": "^4.5",
        "doctrine/doctrine-bundle": "^2.13",
        "doctrine/doctrine-migrations-bundle": "^3.4",
        "doctrine/orm": "^3.3",
        "dragonmantank/cron-expression": "^3.4",
        "dunglas/doctrine-json-odm": "^1.4",
        "ext-ctype": "*",
        "ext-gd": "*",
        "ext-iconv": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "ext-pgsql": "*",
        "gedmo/doctrine-extensions": "^3.18",
        "jdenticon/jdenticon": "^1.0",
        "jfcherng/php-diff": "^6.16",
        "liip/imagine-bundle": "^2.13",
        "pagerfanta/doctrine-orm-adapter": "^4.7",
        "pagerfanta/twig": "^4.7",
        "php": ">=8.4",
        "stof/doctrine-extensions-bundle": "^1.13",
        "symfony/asset": "7.3.*",
        "symfony/asset-mapper": "7.3.*",
        "symfony/console": "7.3.*",
        "symfony/css-selector": "7.3.*",
        "symfony/doctrine-messenger": "^7.3",
        "symfony/dom-crawler": "7.3.*",
        "symfony/dotenv": "7.3.*",
        "symfony/flex": "^2",
        "symfony/form": "7.3.*",
        "symfony/framework-bundle": "7.3.*",
        "symfony/http-client": "7.3.*",
        "symfony/intl": "7.3.*",
        "symfony/mailer": "7.3.*",
        "symfony/mercure-bundle": "^0.3.9",
        "symfony/messenger": "7.3.*",
        "symfony/mime": "7.3.*",
        "symfony/monolog-bundle": "^3.10",
        "symfony/runtime": "7.3.*",
        "symfony/scheduler": "^7.3",
        "symfony/security-bundle": "7.3.*",
        "symfony/serializer": "7.3.*",
        "symfony/stimulus-bundle": "^2.23",
        "symfony/string": "7.3.*",
        "symfony/translation": "7.3.*",
        "symfony/twig-bundle": "7.3.*",
        "symfony/uid": "7.3.*",
        "symfony/ux-dropzone": "^2.23",
        "symfony/ux-icons": "^2.23",
        "symfony/ux-live-component": "^2.23",
        "symfony/ux-turbo": "^2.23",
        "symfony/ux-twig-component": "^2.17",
        "symfony/validator": "7.3.*",
        "symfony/yaml": "7.3.*",
        "symfonycasts/dynamic-forms": "^0.1",
        "symfonycasts/reset-password-bundle": "^1.23",
        "symfonycasts/tailwind-bundle": "^0.7.0",
        "twig/extra-bundle": "^3.20",
        "twig/intl-extra": "^3.20",
        "twig/string-extra": "^3.20",
        "twig/twig": "^3.20",
        "webmozart/assert": "^1.11",
        "xutim/domain": "dev-main"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^8.2",
        "doctrine/doctrine-fixtures-bundle": "^4.0",
        "friendsoftwig/twigcs": "^6.5",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-doctrine": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0",
        "phpstan/phpstan-strict-rules": "^2.0",
        "phpstan/phpstan-symfony": "^2.0",
        "phpstan/phpstan-webmozart-assert": "^2.0",
        "phpunit/phpunit": "^9.6",
        "symfony/browser-kit": "7.3.*",
        "symfony/debug-bundle": "7.3.*",
        "symfony/maker-bundle": "^1.62",
        "symfony/phpunit-bridge": "7.3.*",
        "symfony/stopwatch": "7.3.*",
        "symfony/web-profiler-bundle": "7.3.*",
        "symplify/easy-coding-standard": "^12.5",
        "symplify/monorepo-builder": "^11.2",
        "vincentlanglet/twig-cs-fixer": "^3.5"
    },
    "replace": {
        "xutim/domain": "self.version",
        "xutimcms/core-bundle": "self.version",
        "xutimcms/domain": "self.version",
        "xutimcms/event-bundle": "self.version",
        "xutimcms/redirect-bundle": "self.version",
        "xutimcms/redirect-component": "self.version",
        "xutimcms/security-bundle": "self.version",
        "xutimcms/security-component": "self.version",
        "xutimcms/snippet-bundle": "self.version"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Xutim\\": "src/Xutim/",
            "Xutim\\CoreBundle\\": "src/Xutim/Bundle/CoreBundle/src/",
            "Xutim\\Domain\\": "src/Xutim/Domain/src/",
            "Xutim\\EventBundle\\": "src/Xutim/Bundle/EventBundle/src/",
            "Xutim\\RedirectBundle\\": "src/Xutim/Bundle/RedirectBundle/src/",
            "Xutim\\RedirectComponent\\": "src/Xutim/Component/RedirectComponent/src/",
            "Xutim\\SecurityBundle\\": "src/Xutim/Bundle/SecurityBundle/src/",
            "Xutim\\SecurityComponent\\": "src/Xutim/Component/SecurityComponent/src/",
            "Xutim\\SnippetBundle\\": "src/Xutim/Bundle/SnippetBundle/src/"
        },
        "exclude-from-classmap": [
            "src/Xutim/Bundle/CoreBundle/Tests/",
            "src/Xutim/Bundle/EventBundle/Tests/",
            "src/Xutim/Bundle/RedirectBundle/Tests/",
            "src/Xutim/Bundle/SecurityBundle/Tests/",
            "src/Xutim/Bundle/SnippetBundle/Tests/",
            "src/Xutim/Component/RedirectComponent/Tests/",
            "src/Xutim/Component/SecurityComponent/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Xutim\\CoreBundle\\Tests\\": "src/Xutim/Bundle/CoreBundle/tests/",
            "Xutim\\EventBundle\\Tests\\": "src/Xutim/Bundle/EventBundle/tests/",
            "Xutim\\RedirectBundle\\Tests\\": "src/Xutim/Bundle/RedirectBundle/tests/",
            "Xutim\\RedirectComponent\\Tests\\": "src/Xutim/Component/RedirectComponent/tests/",
            "Xutim\\SecurityBundle\\Tests\\": "src/Xutim/Bundle/SecurityBundle/tests/",
            "Xutim\\SecurityComponent\\Tests\\": "src/Xutim/Component/SecurityComponent/tests/",
            "Xutim\\SnippetBundle\\Tests\\": "src/Xutim/Bundle/SnippetBundle/tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": false,
            "symfony/runtime": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.3.*"
        }
    }
}
